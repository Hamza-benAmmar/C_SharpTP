@{
	ViewData["Title"] = "Home page - liste des etudiants";
}
<h1>Student Cards</h1>
@model IEnumerable<Student>
<div class="row">
@{ 
    foreach (Student s in Model)
    {
        <div class="card" style="width: 18rem;">
            <div class="card-body">
            <h5 class="card-title">@s.first_name @s.last_name</h5>
            <h6 class="card-subtitle mb-2 text-muted">tel : @s.phone_number</h6>
            <h6 class="card-subtitle mb-2 text-muted">University : @s.university</h6>
            <h6 class="card-subtitle mb-2 text-muted">University : @s.timestamp</h6>
            <h6 class="card-subtitle mb-2 text-muted">Course : @s.course</h6>
        </div>
        </div>
    }
 }
 </div>